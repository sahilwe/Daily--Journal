<%- include('partials/header'); -%>

<!-- <h1>Home</h1> -->
<!-- < -->

<% posts.forEach(function(post){%>
<%  if(post.title != "Home"){%>
  <h1><%=post.title%></h1>

  <p><%=post.content.substring(0, 120)+"...."%><a href="/post/<%=post.title%>" >Read more</a></p>
<%}else{%>
  <h1>Home</h1>
  <p><%=StartingContent%></p>
<%}%>
<%})%>
<%-include('partials/footer');-%>
